<project name="MegaPlay JNI" default="-pre-build" basedir=".">
<property file="../fts-linux-demo.local.properties"/>

<target name="-pre-build">
    <exec executable="make" failonerror="true" dir="jni"/>
</target>

<target name="clean">
    <exec executable="make" failonerror="true" dir="jni">
        <arg value="clean"/>
    </exec>
</target>
</project>