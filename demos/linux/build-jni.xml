<project name="build-fts-linux-demo-jni" default="-pre-build" basedir=".">
<property file="../../../fts-linux-demo.local.properties"/>

<target name="build-jni">
    <exec executable="make" failonerror="true" dir="jni"/>
</target>

<target name="clean-jni">
    <exec executable="make" failonerror="true" dir="jni">
        <arg value="clean"/>
    </exec>
</target>
</project>