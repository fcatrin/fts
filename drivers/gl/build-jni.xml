<?eclipse.ant.import?>
<project name="FTS GL JNI" default="-pre-build" basedir=".">
<property file="../../../fts-gl.local.properties"/>

<target name="build-jni">
    <exec executable="make" failonerror="true" dir="jni"/>
</target>

<target name="clean-jni">
    <exec executable="make" failonerror="true" dir="jni">
        <arg value="clean"/>
    </exec>
</target>
</project>